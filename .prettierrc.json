{
  "printWidth": 150,
  "useTabs": true,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "tabWidth": 4,
  "importOrder": [
    "^src/lib/(.*).svelte$",
    "^src/lib/(.*)$",
    "^src/defaults/(.*)$",
    "^src/stores/(.*)$",
    "^src/types/(.*)$",
    "^src/helpers/(.*)$",
    "^src/layers/(.*)$",
    "^src/panels/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
